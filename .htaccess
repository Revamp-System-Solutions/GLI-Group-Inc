Options +SymLinksIfOwnerMatch
RewriteEngine On

# Allow Installatron requests
RewriteCond %{REQUEST_FILENAME} deleteme\.\w+\.php
RewriteRule (.*) - [L] 

RewriteRule ^ index.php [L]
RewriteCond %{HTTP_HOST} ^builders\.gligroupinc\.com$
RewriteRule ^/?$ "https\:\/\/builders\.gligroupinc\.com\/\#builders" [R=301,L]

